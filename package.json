{
  "name": "nexus-pro",
  "version": "1.0.0",
  "description": "AI Co-Creation Platform - The Ultimate Development Environment",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "backend": "cd backend && python main.py",
    "start": "concurrently \"npm run backend\" \"npm run dev\"",
    "test": "vitest",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod",
    "docker:build": "docker build -t nexus-pro .",
    "docker:run": "docker run -p 3000:3000 -p 8000:8000 nexus-pro"
  },
